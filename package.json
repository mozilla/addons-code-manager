{
  "name": "addons-code-manager",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "23.3.13",
    "@types/node": "10.12.20",
    "@types/react": "16.7.22",
    "@types/react-bootstrap": "0.32.16",
    "@types/react-dom": "16.0.11",
    "@types/react-redux": "7.0.1",
    "@types/react-router-dom": "4.3.1",
    "@types/redux-logger": "3.0.6",
    "bootstrap": "4.2.1",
    "express": "4.16.4",
    "http-proxy-middleware": "0.19.1",
    "node-sass": "4.11.0",
    "react": "16.7.0",
    "react-bootstrap": "1.0.0-beta.5",
    "react-diff-view": "2.0.0-alpha.1",
    "react-dom": "16.7.0",
    "react-redux": "5.1.1",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.1.3",
    "redux": "4.0.1",
    "redux-devtools-extension": "2.13.7",
    "redux-logger": "3.0.6",
    "typesafe-actions": "3.0.0",
    "typescript": "3.2.4",
    "universal-cookie-express": "3.0.7"
  },
  "devDependencies": {
    "@storybook/react": "^4.1.11",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/storybook__react": "^4.0.0",
    "@types/supertest": "^2.0.7",
    "babel-loader": "8.0.4",
    "chalk": "^2.4.2",
    "codecov": "^3.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "http-proxy-response-rewrite": "^0.0.1",
    "jest-enzyme": "^7.0.1",
    "jest-fetch-mock": "^2.1.0",
    "prettier": "1.16.3",
    "pretty-quick": "^1.8.1",
    "stmux": "^1.6.1",
    "supertest": "^3.4.2",
    "type-coverage": "^1.8.0"
  },
  "scripts": {
    "build": "react-scripts build",
    "build-local-dev-config": "cat .env.common-local .env.dev > .env.local",
    "dev": "yarn build-local-dev-config && yarn stmux [ 'yarn test' : 'tsc -w' : 'yarn react-scripts-start' : 'yarn serve-dev' ]",
    "dev-2col": "yarn build-local-dev-config && yarn stmux [ 'yarn test' .. [ 'tsc -w' : 'yarn react-scripts-start' : 'yarn serve-dev' ] ]",
    "dev-servers": "yarn build-local-dev-config && yarn stmux [ 'yarn serve-dev' : 'yarn react-scripts-start' ]",
    "eject": "react-scripts eject",
    "prettier": "prettier --write '**'",
    "prettier-ci": "prettier --list-different '**' || (echo '\n\nThis failure means you did not run `yarn prettier-dev` before committing\n\n' && exit 1)",
    "prettier-dev": "pretty-quick --branch master",
    "react-scripts-start": "PORT=3100 BROWSER=none react-scripts start",
    "serve": "node scripts/server.js",
    "serve-dev": "NODE_ENV=development yarn serve",
    "stage": "cat .env.common-local .env.stage > .env.local && yarn start-dev",
    "start": "yarn build && NODE_ENV=production yarn serve",
    "start-local-dev": "yarn build-local-dev-config && yarn start",
    "start-local-prod": "cat .env.common-local > .env.local && yarn start",
    "start-local-stage": "cat .env.common-local .env.stage > .env.local && yarn start",
    "stmux": "stmux -nM -t addons-code-manager -e 'error,!0 errors'",
    "test": "react-scripts test",
    "test-ci": "yarn test --coverage && codecov",
    "type-coverage": "type-coverage",
    "typecheck": "tsc",
    "storybook": "start-storybook -p 9001 -c stories/setup"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "typeCoverage": {
    "atLeast": 99
  },
  "homepage": "/"
}
